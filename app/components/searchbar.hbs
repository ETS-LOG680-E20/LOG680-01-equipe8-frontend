<h2>Search parameters</h2>
  <div class="row">
  
  <div class="input-group col-sm">
	  <div class="input-group-prepend">
		<span class="input-group-text" id="basic-addon1">Issue ID</span>
	  </div>
    {{input value=searchIssueId}}
  </div>
  
  <div class="input-group col-sm">
	  <div class="input-group-prepend">
		<span class="input-group-text" id="basic-addon1">Start date</span>
	  </div>
	  <EmberFlatpickr
			@onChange={{action (mut this.searchStartDate)}}
		  />
	</div>
	
	  <div class="input-group col-sm">
	  <div class="input-group-prepend">
		<span class="input-group-text" id="basic-addon1">End date</span>
	  </div>
	  <EmberFlatpickr
        @onChange={{action (mut this.searchEndDate)}}
      />
	</div>
	
  <div class="col-sm">
    <button type="button" class="btn btn-primary" {{action "getSearchParams" on="click"}}>Search</button>
  </div>
</div>