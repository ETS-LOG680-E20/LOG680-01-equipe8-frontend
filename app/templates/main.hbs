
<div class="container">
  <h2>Time interval</h2>
  <div class="row">
    <div class="col-sm">
      Start date:
      <EmberFlatpickr
        @defaultDate={{this.defaultDate}}
        @onChange={{action (mut this.startDate)}}
      />
    </div>
    <div class="col-sm">
      End date:
      <EmberFlatpickr
        @defaultDate={{this.defaultDate}}
        @onChange={{action (mut this.endDate)}}
      />
    </div>
    <div class="col-sm">
      <button type="button" class="btn btn-primary">Search</button>
    </div>
  </div>
<hr>
  <h2>Issues per column</h2>
  <div class="row">
    <ColumnIssues @columnName="SomeName" @numberOfIssues="150">
    </ColumnIssues>
    <ColumnIssues @columnName="SomeOtherName" @numberOfIssues="20">
    </ColumnIssues>
    <ColumnIssues @columnName="ThirdCol" @numberOfIssues="15">
    </ColumnIssues>
    <ColumnIssues @columnName="LastCol" @numberOfIssues="3">
    </ColumnIssues>
  </div>
  <hr>
